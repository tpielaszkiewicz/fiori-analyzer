          "@sap.quickinfo": "Item number of the SD document"
        },
        "Description": {
          "type": "cds.String",
          "length": 40,
          "@sap.label": "Name",
          "@sap.quickinfo": "Text, 40 Characters Long"
        },
        "Material": {
          "type": "cds.String",
          "length": 18,
          "@sap.display.format": "UpperCase",
          "@sap.text": "to_material/Material_Text",
          "@sap.label": "Material",
          "@sap.quickinfo": "Material Number",
          "@sap.value.list": "standard"
        },
        "Quantity": {
          "type": "cds.Decimal",
          "precision": 15,
          "scale": 3,
          "@sap.unit": "UnitOfMeasure",
          "@sap.label": "Quantity"
        },
        "UnitOfMeasure": {
          "type": "cds.String",
          "length": 3,
          "@sap.label": "Internal UoM",
          "@sap.quickinfo": "Unit of Measurement",
          "@sap.semantics": "unit-of-measure"
        },
        "NetValue": {
          "type": "cds.Decimal",
          "precision": 16,
          "scale": 3,
          "@sap.unit": "Currency",
          "@sap.label": "Net Value",
          "@sap.quickinfo": "Net Value in Document Currency"
        },
        "Currency": {
          "type": "cds.String",
          "length": 5,
          "@sap.label": "Currency",
          "@sap.quickinfo": "Currency Key",
          "@sap.semantics": "currency-code"
        },
        "TaxRate": {
          "type": "cds.Decimal",
          "precision": 23,
          "scale": 4,
          "@sap.label": "Tax Rate",
          "@sap.quickinfo": "Tax Rate in Percent"
        },
        "to_header": {
          "@cds.ambiguous": "missing on condition?",
          "type": "cds.Association",
          "target": "zorders00.Orders",
          "keys": []
        },
        "to_material": {
          "@cds.ambiguous": "missing on condition?",
          "type": "cds.Association",
          "target": "zorders00.MaterialVH",
          "on": [
            {
              "ref": [
                "to_material",
                "Material"
              ]
            },
            "=",
            {
              "ref": [
                "Material"
              ]
            }
          ]
        }
      }
    },
