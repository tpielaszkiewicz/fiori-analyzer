    "zorders00.Orders": {
      "kind": "entity",
      "@cds.persistence.skip": true,
      "@sap.creatable": "false",
      "@sap.updatable": "false",
      "@sap.deletable": "false",
      "@sap.searchable": "true",
      "@sap.content.version": "1",
      "@sap.label": "BTP Bootcamp order header (consumption view)",
      "elements": {
        "OrderId": {
          "key": true,
          "type": "cds.UUID",
          "@sap.label": "BTP Order ID"
        },
        "Customer": {
          "type": "cds.String",
          "length": 10,
          "@sap.display.format": "UpperCase",
          "@sap.text": "to_customer/CustomerName",
          "@sap.label": "Customer",
          "@sap.quickinfo": "Customer Number",
          "@sap.value.list": "standard"
        },
        "Description": {
          "type": "cds.String",
          "length": 40,
          "@sap.label": "Order description"
        },
        "CreateDate": {
          "type": "cds.Date",
          "@sap.display.format": "Date",
          "@sap.label": "Created on",
          "@sap.quickinfo": "Date on which the record was created"
        },
        "CreatedBy": {
          "type": "cds.String",
          "length": 12,
          "@sap.display.format": "UpperCase",
          "@sap.label": "User Name"
        },
        "SalesOrganisation": {
          "type": "cds.String",
          "length": 4,
          "@sap.display.format": "UpperCase",
          "@sap.text": "to_salesOrg/SalesOrganization_Text",
          "@sap.label": "Sales Organization",
          "@sap.value.list": "standard"
        },
        "OrderType": {
          "type": "cds.String",
          "length": 4,
          "@sap.display.format": "UpperCase",
          "@sap.text": "to_orderType/OrderType_Text",
          "@sap.label": "Sales Document Type",
          "@sap.value.list": "standard"
        },
        "Status": {
          "type": "cds.String",
          "length": 1,
          "@sap.display.format": "UpperCase",
          "@sap.text": "to_orderStatus/Status_Text",
          "@sap.label": "Overall Status",
          "@sap.quickinfo": "Overall Processing Status (Header/All Items)",
          "@sap.value.list": "standard"
        },
        "NetValue": {
          "type": "cds.Decimal",
          "precision": 16,
          "scale": 3,
          "@sap.unit": "Currency",
          "@sap.label": "Net Value",
          "@sap.quickinfo": "Net Value in Document Currency"
        },
        "Currency": {
          "type": "cds.String",
          "length": 5,
          "@sap.label": "Currency",
          "@sap.quickinfo": "Currency Key",
          "@sap.semantics": "currency-code"
        },
        "to_customer": {
          "@cds.ambiguous": "missing on condition?",
          "type": "cds.Association",
          "target": "zorders00.CustomerVH",
          "on": [
            {
              "ref": [
                "to_customer",
                "Customer"
              ]
            },
            "=",
            {
              "ref": [
                "Customer"
              ]
            }
          ]
        },
        "to_item": {
          "@cds.ambiguous": "missing on condition?",
          "type": "cds.Association",
          "target": "zorders00.OrderItems",
          "cardinality": {
            "max": "*"
          },
          "keys": []
        },
        "to_orderStatus": {
          "@cds.ambiguous": "missing on condition?",
          "type": "cds.Association",
          "target": "zorders00.OrderStatusVH",
          "on": [
            {
              "ref": [
                "to_orderStatus",
                "Status"
              ]
            },
            "=",
            {
              "ref": [
                "Status"
              ]
            }
          ]
        },
        "to_orderType": {
          "@cds.ambiguous": "missing on condition?",
          "type": "cds.Association",
          "target": "zorders00.OrderTypeVH",
          "on": [
            {
              "ref": [
                "to_orderType",
                "OrderType"
              ]
            },
            "=",
            {
              "ref": [
                "OrderType"
              ]
            }
          ]
        },
        "to_salesOrg": {
          "@cds.ambiguous": "missing on condition?",
          "type": "cds.Association",
          "target": "zorders00.SalesOrgVH",
          "on": [
            {
              "ref": [
                "to_salesOrg",
                "SalesOrganization"
              ]
            },
            "=",
            {
              "ref": [
                "SalesOrganisation"
              ]
            }
          ]
        }
      }
    },
    "zorders00.OrderItems": {
      "kind": "entity",
      "@cds.persistence.skip": true,
      "@sap.creatable": "false",
